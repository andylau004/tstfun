 <blog4go minlevel="info">
	<filter levels="trace">
		<rotatefile path="./oldlogs/trace.log" type="time" retentions="5"></rotatefile>
	</filter>
	<filter levels="debug">
		<rotatefile path="./oldlogs/debug.log" type="time" retentions="5"></rotatefile>
	</filter>
	<filter levels="info">
	<!-- size=50M -->
		<!-- <rotatefile path="./logs/info.log" type="size" rotateSize="50000000" type="time" retentions="10"></rotatefile> -->
		<rotatefile path="./oldlogs/info.log" type="time" retentions="5"></rotatefile>
	</filter>
	<filter levels="error">
		<rotatefile path="./oldlogs/info.log" type="time" retentions="5"></rotatefile>
	</filter>
	<filter levels="critical">
		<socket network="udp" address="127.0.0.1:12124"></socket>
	</filter>
</blog4go>




